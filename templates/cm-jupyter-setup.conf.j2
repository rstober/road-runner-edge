modules:
  jupyter:
    ConfigurableHTTPProxy:
      api_port: 8902
    JupyterHub:
      hub_port: 8901
      port: 8000
    additional_hub_admins: []
    additional_hub_domains:
    - jupyterhub.brightcomputing.com
    - '*.jupyterhub.com'
    - '*.eth.cluster'
    - eth.cluster
    c.JupyterHub.data_files_path: /cm/shared/apps/jupyter/current/share/jupyterhub
    c.JupyterHub.hub_ip: 0.0.0.0
    c.JupyterHub.spawner_class: '"jupyterhub.spawner.LocalProcessSpawner"'
    c.JupyterHub.ssl_cert: /cm/local/apps/jupyter/current/conf/certs/my_ssl.cert
    c.JupyterHub.ssl_key: /cm/local/apps/jupyter/current/conf/certs/my_ssl.key
    c.PAMAuthenticator.open_sessions: 'False'
    c.Spawner.cmd: '[''jupyterhub-singleuser-gw'']'
    c.Spawner.default_url: '"/lab"'
    c.Spawner.env_keep: '["PATH", "LANG", "LC_ALL", "PYTHONPATH",
      "MODULEPATH"]'
    ca_certs.jupyterhub.key: /cm/local/apps/jupyter/current/conf/certs/my_sslca.key
    ca_certs.jupyterhub.pem: /cm/local/apps/jupyter/current/conf/certs/my_sslca.cert
    configuration_overlay_name: jupyterhub
    configuration_overlay_priority: 500
    configuration_overlay_remove: []
    local_packages:
    - cm-jupyter-local
    login_nodes:
    - {{ ansible_hostname }}
    shared_packages:
    - cm-jupyter
    skip_packages: false
